{
  "name": "@tunnckocore/jest-runner-babel",
  "version": "4.0.2",
  "description": "Fork of the original Jest Runner for Babel and a lot better, plays nicely with monorepos",
  "repository": "https://github.com/tunnckoCore/opensource/tree/master/@tunnckocore/jest-runner-babel",
  "homepage": "https://tunnckocore.com/opensource",
  "author": "Charlike Mike Reagent <opensource@tunnckocore.com> (https://tunnckocore.com)",
  "license": "MPL-2.0",
  "engines": {
    "node": ">=10.13"
  },
  "main": "src/index.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "files": [
    "src"
  ],
  "keywords": [
    "bin",
    "binaries",
    "binary",
    "child",
    "concurrency",
    "develop",
    "exec",
    "execa",
    "execfile",
    "execute",
    "file",
    "fork",
    "local",
    "npm",
    "parallel",
    "path",
    "process",
    "sequence",
    "series",
    "shell",
    "spawn",
    "tunnckocorehq"
  ],
  "scripts": {},
  "peerDependencies": {
    "@babel/core": "^7.6.0"
  },
  "dependencies": {
    "@tunnckocore/create-jest-runner": "^1.0.2",
    "cosmiconfig": "^5.2.1"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "jest": {
    "coverageThreshold": {
      "src/**/*.js": {
        "statements": 17,
        "branches": 0,
        "functions": 0,
        "lines": 17
      }
    }
  },
  "jestCov": {
    "value": 8.93,
    "color": "red"
  },
  "licenseStart": 2019,
  "verb": {
    "readme": "../../readme-template.md",
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 4
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": []
  }
}