{
  "name": "@tunnckocore/eslint-config",
  "version": "5.4.9",
  "licenseStart": 2019,
  "license": "MPL-2.0",
  "description": "Shareable ESLint config for all @tunnckoCore projects",
  "author": "Charlike Mike Reagent <opensource@tunnckocore.com> (https://tunnckocore.com)",
  "homepage": "https://tunnckocore.com/opensource",
  "funding": [
    "https://ko-fi.com/tunnckoCore/commissions",
    "https://github.com/sponsors/tunnckoCore",
    "https://patreon.com/tunnckoCore"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tunnckoCore/opensource.git",
    "directory": "@configs/eslint-config"
  },
  "main": "index.js",
  "files": [
    "*.js"
  ],
  "engines": {
    "node": ">=10.13"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "scripts": {},
  "dependencies": {
    "@tunnckocore/utils": "^1.3.8",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-eslint": "^10.0.3",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-mdx": "^1.6.8",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mdx": "^1.6.8",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-unicorn": "^15.0.1"
  },
  "peerDependencies": {
    "eslint": "^6.8.0",
    "prettier": "^2.0.2"
  },
  "cov": {
    "color": "grey"
  },
  "keywords": [
    "airbnb codestyle",
    "babel-preset",
    "browser config",
    "browser-config",
    "browserslist",
    "browserslist-config",
    "browserslist-preset",
    "chrome",
    "config",
    "devest",
    "env",
    "eslint-config",
    "eslint-plugin",
    "esmodules",
    "evergreen",
    "hela",
    "jsx",
    "preset-env",
    "prettier",
    "prettier-config",
    "promises",
    "react",
    "react hooks",
    "shareable-configs",
    "tunnckocorehq",
    "typescript",
    "typescript-eslint",
    "unicorn",
    "xo",
    "xo-config"
  ],
  "verb": {
    "readme": "../../readme-template.md",
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 4
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": [],
    "related": {
      "list": []
    }
  }
}
